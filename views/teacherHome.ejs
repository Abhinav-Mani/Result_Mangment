<%- include('includes/head.ejs') %>    
<%- include('includes/navbar.ejs') %>

<div class="container">
    <div class="card" style="margin-bottom: 20px;" >
        <div class="card-body">
          Your Courses
        </div>
      </div>

    <% courses.forEach((course)=>{ %>
      <div class="card">
        <div class="card-body">
        <a href="/teacher/<%=course.id%>">
          <%= course.name %>(<%= course.id %>)
        </a>
        </div>
      </div>
    <% }) %>

</div>

